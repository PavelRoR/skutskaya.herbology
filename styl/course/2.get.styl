#get
	padding 50px 0
	background-color: #FBFAF8
	background-image url(../img/backgrounds/get_bg.png)
	background-repeat no-repeat
	background-position: center
	background-size: cover
.get-list
	width 100%
	padding-left: 340px
	background-image url(../img/images/get_img.png)
	background-repeat no-repeat
	background-position: center left
	background-size 300px
	li
		list-style: none
		padding-left: 40px
		font-size: 23px
		background-image url(../img/icons/grass.png)
		background-repeat no-repeat
		background-position: left top 2px
		margin-bottom: 20px
		&:last-child
			margin-bottom: 0